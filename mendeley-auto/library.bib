isbn = {9781467388504},
issn = {10636919},
journal = {Proceedings of the IEEE Computer Society Conference on Computer Vision and Pattern Recognition},
pages = {5515--5524},
title = {{Deep stereo: Learning to predict new views from the world's imagery}},
