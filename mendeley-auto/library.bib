issn = {2329-4302},
journal = {Journal of Medical Imaging},
month = {dec},
number = {4},
pages = {044506},
title = {{LUNGx Challenge for computerized lung nodule classification}},
url = {http://medicalimaging.spiedigitallibrary.org/article.aspx?doi=10.1117/1.JMI.3.4.044506},
volume = {3},
year = {2016}
}
