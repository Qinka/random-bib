pages = {5822--5830},
title = {{Low-Shot Learning with Imprinted Weights}},
year = {2018}
}
@article{Kim2021,
abstract = {Deep learning (DL) is an advanced machine learning approach used in diverse areas such as bioinformatics, image analysis, and natural language processing. Here, using brain magnetic resonance imaging (MRI) data obtained at early stages of infarcts, we attempted to develop a convolutional neural network (CNN) to predict the ambulatory outcome of corona radiata infarction at six months after onset. We retrospectively recruited 221 patients with corona radiata infarcts. A favorable outcome of ambulatory function was defined as a functional ambulation category (FAC) score of ≥ 4 (able to walk without a guardian's assistance), and a poor outcome of ambulatory function was defined as an FAC score of {\textless} 4. We used a CNN algorithm. Of the included subjects, 69.7{\%} (n = 154) were assigned randomly to the training set and the remaining 30.3{\%} (n = 67) were assigned to the validation set to measure the model performance. The area under the curve was 0.751 (95{\%} CI 0.649–0.852) for the prediction of ambulatory function with the validation dataset using the CNN model. We demonstrated that a CNN model trained using brain MRIs captured at an early stage after corona radiata infarction could be helpful in predicting long-term ambulatory outcomes.},
author = {Kim, Jeoung Kun and Choo, Yoo Jin and Shin, Hyunkwang and Choi, Gyu Sang and Chang, Min Cheol},
doi = {10.1038/s41598-021-87176-0},
issn = {2045-2322},
journal = {Scientific Reports},
month = {dec},
number = {1},
pages = {7989},
title = {{Prediction of ambulatory outcome in patients with corona radiata infarction using deep learning}},
url = {http://www.nature.com/articles/s41598-021-87176-0},
volume = {11},
year = {2021}
}
